            <div  id="forma1"></div>
        </div>

    </div>


    <!--Hero Area End-->

    <div class="container "  >
        <div  class="row justify-content-center forma my-5">
            <form method="get" action="/registracija_vozila/registruj">

                <div class="form-group">
                    <label for="exampleFormControlInput1">Ime i Prezime</label>
                    <input type="text" class="form-control" id="exampleFormControlInput1"
                           placeholder="Petar Petrović" name="ImePrezime">
                </div>

                <div class="form-group">
                    <label for="exampleFormControlInput1">Marka i Model Vozila</label>
                    <input type="text" class="form-control" id="exampleFormControlInput1"
                           placeholder="Fiat Punto " name="MarkaModel">
                </div>

                <div class="form-group"> <label for="exampleFormControlTextarea1">Adresa</label><br>

                    <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="Adresa"></textarea>
                </div>

                <div class="form-group">
                    <label for="exampleFormControlSelect1">Vrsta Vozila</label><br>
                    <select class="form-control" id="exampleFormControlSelect1" name="Vrsta">
                        <option value="Putničko">Putničko</option>
                        <option value="Teretno">Teretno</option>
                        <option value="Motorcikl">Motorcikl</option>
                        <option value="Autobus">Autobus</option>
                        <option value="Priključno">Priključno</option>
                    </select>
                </div><br>
                <div class="form-group">
                    <label for="exampleFormControlSelect1">Godište</label><br>
                    <select class="form-control" id="exampleFormControlSelect1"  name="Godiste">
                        <%for(var i =2020; i>1960; i--){%>
                        <%if(i==parseInt(data.godiste)){%>
                        <option value="<%=i%>" selected><%=i%></option>
                        <%}else{%>
                            <option value="<%=i%>"><%=i%></option>
                        <%}%>
                        <%}%>

                    </select>
                </div>
                <br>
                <div class="form-group">
                    <label for="exampleFormControlSelect1">Snaga motora u KW</label><br>
                    <select class="form-control" id="exampleFormControlSelect1" name="Snaga">
                        <%for(var i=0; i <hp.length; i++){%>

                        <option value="<%=hp[i]%>"><%=hp[i]%>  </option>
                        <%}%>

                    </select>
                </div><br>

                <div class="form-group">
                    <label for="exampleFormControlInput1">Zapremina motora</label>
                    <input type="text" class="form-control" id="exampleFormControlInput1"
                           placeholder="" name="Zapremina" value=<%=data.zapremina%>>
                </div>

                <div class="form-group">
                    <label for="exampleFormControlInput1">Tablice</label>
                    <input type="text" class="form-control" id="exampleFormControlInput1"
                           placeholder="" name="Tablice" value=<%=data.tablice%>>
                </div><br>

                <button type="submit" class="btn">Registruj</button>
            </form>

        </div>
        <div class="row justify-content-center my-5 py-5">
            <h2><%=warning.warning%></h2>
        </div>
    </div>
