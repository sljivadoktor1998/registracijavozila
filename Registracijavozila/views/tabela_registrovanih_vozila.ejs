    <!--Hero Area End-->

    <div class="container">
        <div class="row justify-content-center forma my-5">

            <table class="table">
                <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Ime i Prezime</th>
                    <th scope="col">Vozilo</th>
                    <th scope="col">Datum Registracije</th>
                    <th scope="col">Cena</th>
                    <th scope="col">Reg. Oznaka</th>
                </tr>
                </thead>
                <tbody>
                <%for(var i =0; i <auto.length; i++){%>
                <% var datum = new Date(auto[i].datumVazenja);
                    var day,month,year;
                    day=datum.getDate();
                    month=datum.getMonth()+1;
                    year=datum.getFullYear()-1;
                    datum=year+'-'+ month+'-'+day;
                %>
                <tr>
                    <th scope="row"><%=i+1%></th>
                    <td><%=auto[i].Ime_Prezime%></td>
                    <td><%=auto[i].Marka_Model%></td>
                    <td><%=datum%></td>
                    <th scope="col"><%=auto[i].Cena%></th>
                    <td><%=auto[i].tablice%></td>
                </tr>
                <%}%>

                </tbody>
            </table>
        </div>

    </div>
